# LinkedAgent Environment Configuration

# API Configuration
VITE_API_BASE_URL=https://api.linkedagent.com/v1
VITE_USE_MOCK_API=true

# WebSocket Configuration
VITE_WS_BASE_URL=wss://api.linkedagent.com/v1

# Authentication
VITE_JWT_SECRET=your-jwt-secret-key-here

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760  # 10MB in bytes
VITE_ALLOWED_FILE_TYPES=.xlsx,.xls,.csv

# Rate Limiting
VITE_API_RATE_LIMIT=100  # requests per minute

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_NOTIFICATIONS=true

# Monitoring & Logging
VITE_SENTRY_DSN=your-sentry-dsn-here
VITE_LOG_LEVEL=info

# Development
VITE_DEV_MODE=true
VITE_MOCK_DELAYS=true  # Add artificial delays to mock API calls

# Production
VITE_APP_VERSION=0.1.0
VITE_BUILD_DATE=2024-01-20